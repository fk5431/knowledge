  <target name="compile.module.movie" depends="compile.module.movie.production,compile.module.movie.tests" description="Compile module movie"/>
  
  <target name="compile.module.movie.production" depends="register.custom.compilers" description="Compile module movie; production classes">
    <mkdir dir="${movie.output.dir}"/>
    <javac2 destdir="${movie.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.movie}/javac">
      <compilerarg line="${compiler.args.movie}"/>
      <bootclasspath refid="movie.module.bootclasspath"/>
      <classpath refid="movie.module.production.classpath"/>
      <src refid="movie.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.movie"/>
    </javac2>
    
    <copy todir="${movie.output.dir}">
      <fileset dir="${module.movie.basedir}/../src/main/java">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.movie.basedir}/../src/main/resources">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="compile.module.movie.tests" depends="register.custom.compilers,compile.module.movie.production" description="compile module movie; test classes" unless="skip.tests"/>
  
  <target name="clean.module.movie" description="cleanup module">
    <delete dir="${movie.output.dir}"/>
    <delete dir="${movie.testoutput.dir}"/>
  </target>
